      <section id="id3339993">
        <title>Can you remember?</title>
        <figure id="id6553535">
          <media id="id6553535_media" alt="">
            <image mime-type="image/png" src="../../media/graphics1-0ddb.png" id="id6553535__onlineimage" height="310" width="239"/>
